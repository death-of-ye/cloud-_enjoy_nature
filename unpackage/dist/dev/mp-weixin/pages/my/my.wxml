<view class="my"><block wx:if="{{isRow}}"><view class="{{[isRow?'box box1':'box']}}" style="{{'opacity:'+(opacity)+';'}}"><view class="title">我的</view></view></block><view class="header"><view class="logo" style="{{'margin-top:'+(isTop?168+'rpx':148+'rpx')+';'+('margin-bottom:'+(isTop?121+'rpx':141+'rpx')+';')}}"><view class="left"><image src="{{headPortrait}}" mode lazy-load="{{true}}"></image><text class="nick">{{Nickname}}</text></view><view class="right"><view>修改资料</view></view></view></view><view class="my_nav"><view><block wx:for="{{my_nav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['my_nav_to',['$0'],[[['my_nav','',index,'url']]]]]]]}}" bindtap="__e"><image src="{{item.img}}" mode="heightFix" lazy-load="{{true}}"></image><text>{{item.text}}</text></view></block></view></view><view class="my_order"><view class="f-r"><view class="left">我的订单</view><view class="right">查看全部<image style="height:20rpx;" src="../../static/image/my/READMORE.png" mode="heightFix"></image></view></view><view class="s-r"><block wx:for="{{my_order}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['my_order_to',['$0'],[[['my_order','',index,'url']]]]]]]}}" bindtap="__e"><image src="{{item.img}}" mode="aspectFit" lazy-load="{{true}}"></image><text>{{item.text}}</text></view></block></view></view><block wx:for="{{more}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['more_to',['$0'],[[['more','',index,'url']]]]]]]}}" class="more" bindtap="__e"><view class="left"><image src="{{item.img}}" mode></image>{{''+item.text+''}}</view><view class="right"><image src="../../static/image/my/READMORE.png" mode lazy-load="{{true}}"></image></view></view></block><view style="width:750rpx;height:98rpx;"></view><view class="tab"><block wx:for="{{tab}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navTo',['$0'],[[['tab','',index,'url']]]]]]]}}" class="tab-item" bindtap="__e"><image src="{{item.img}}" mode="heightFix" lazy-load="{{true}}"></image><text>{{item.text}}</text></view></block></view></view>