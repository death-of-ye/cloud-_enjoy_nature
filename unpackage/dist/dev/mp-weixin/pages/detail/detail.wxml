<view class="detail"><uni-nav-bar class="status-title" vue-id="3db16520-1" fixed="{{true}}" statusBar="{{true}}" title="{{title}}" data-event-opts="{{[['^clickLeft',[['back']]]]}}" bind:clickLeft="__e" bind:__l="__l" vue-slots="{{['left']}}"><uni-icons class="status-icon" vue-id="{{('3db16520-2')+','+('3db16520-1')}}" slot="left" type="arrowleft" bind:__l="__l"></uni-icons></uni-nav-bar><view class="banner"><uni-swiper-dot class="swiper" vue-id="3db16520-3" info="{{detailBox.swiperImg}}" current="{{current}}" field="content" mode="{{mode}}" dotsStyles="{{dotsStyles}}" bind:__l="__l" vue-slots="{{['default']}}"><swiper data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" class="swiper-box" bindchange="__e"><block wx:for="{{swiperImg}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="../../static/image/deatil/banner.png" mode></image><view class="CornerMarker">{{current+1+"/"+swiperImg.length}}</view></view></swiper-item></block></swiper></uni-swiper-dot></view><view class="detail-box"><view class="detail-box-first-line"><view class="detail-box-first-line-left"><text class="price-icon">￥<text class="price">{{detailBox.price}}</text><text class="price-company">元</text></text></view><view class="detail-box-first-line-right"><text>月销<text class="number">{{detailBox.num+"+"}}</text></text></view></view><view class="detail-box-second-line"><view class="detail-box-second-line-left"><text class="name">{{detailBox.name}}</text></view><view class="detail-box-second-line-right"><image src="../../static/image/deatil/share.png" mode="heightFix"></image></view></view><view class="detail-box-third-line"><block wx:for="{{detailBox.keyword}}" wx:for-item="item" wx:for-index="__i0__"><text class="pin">{{item.text}}</text></block></view></view><view class="Specifications"><view class="Specifications-first-line"><view class="Specifications-first-line-first-column"><text>选择</text></view><view class="Specifications-first-line-second-column"><text>{{"已选："+Specifications.Specification}}</text></view><view class="Specifications-first-line-third-column"><image src="../../static/image/my/READMORE.png" mode></image></view></view><view class="Specifications-second-line"><view class="Specifications-second-line-first-column"><text>运费</text></view><view class="Specifications-second-line-second-column"><text>在线支付运费(以实际支付为准)</text></view></view></view><view class="evaluate"><block wx:if="{{isEvaluate}}"><view class="evaluate-first-line"><view class="evaluate-first-line-left"><text class="evaluate-first-line-left-title">商品评价(3)</text></view><view class="evaluate-first-line-right"><view data-event-opts="{{[['tap',[['more',['$event']]]]]}}" class="evaluate-first-line-right-more" bindtap="__e"><text>查看更多</text><image src="../../static/image/my/READMORE.png" mode></image></view></view></view></block><block wx:for="{{evaluate}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="evaluate-second-line"><view class="evaluate-second-line-first"><view class="evaluate-second-line-first-left"><image src="{{item.headPortrait}}" mode></image></view><view class="evaluate-second-line-first-right"><view class="evaluate-second-line-first-right-nick"><text>{{item.nick}}</text></view><view class="evaluate-second-line-first-right-time"><text>5天前</text></view></view></view><view class="evaluate-second-line-second"><text>{{item.evaluate}}</text></view></view></block></view><view class="BabyDetails"><view class="BabyDetails-title"><view class="BabyDetails-title-left"></view><view class="BabyDetails-title-center"><text>宝贝详情</text></view><view class="BabyDetails-title-right"></view></view><view class="BabyDetails-body"><image src="{{detailImg}}" mode="widthFix"></image></view></view></view>